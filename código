public class Mensagem {

    public static void obterMensagem(int hora) {
        switch (hora) {
            case 5, 6, 7, 8, 9, 10, 11, 12 -> mensagemBomDia();
        }


    }

    private static void mensagemBomDia() {
        System.out.println("Bom dia!");
    }
}
